<h2>提携詳細</h2>
<canvas id="canvas"><!-- 提携詳細 --></canvas>
<script>
	// <![CDATA[
	jQuery(function($) {
		$("#jQTabs").tabs({
			fx : {
				opacity : 'toggle',
				duration : 'fast'
			}
		});
	});
	// ]]>
</script>
<div class="board">
	<div id="jQTabs">
		<ul>
			<li><a href="#tabPromotion"><span>提携詳細</span></a></li>
			<li><a href="#tabReward"><span>バナー一覧</span></a></li>
		</ul>
		<!-- #tab -->
		<div id="tabPromotion">
			<h3><a href="#section1">提携情報</a></h3>
			<div id="attributes" class="tieUpDetails">
				<dl>
					<dt><%= label :publish, :id %></dt>
					<dd><%= @publish.id %></dd>
					<dt><%= label :publish, :approval_status %></dt>
					<dd><%= @publish.approval_status %></dd>
				</dl>
			</div>
			<% if @publish.approved? %>
				<h3>タグ情報</h3>
				<div id="tag_attributes">
					<pre><%= publish_tag(@publish) %><pre>
				</div>
				<h3>クリックURL</h3>
				<div id="tag_attributes">
					<pre><%= publish_click_url(@publish) %></pre>
				</div>
			<% end %>
			<h3>プロモーション詳細</h3>
			<div id="promotion_attributes" class="promotionDetails">
				<dl>
					<dt><%= label :promotion, :name %></dt>
					<dd><%= @publish.promotion.name %></dd>
					<dt><%= label :promotion,	:url %></dt>
					<dd><%= @publish.promotion.url %></dd>
				</dl>
				<dl class="aspName">
					<dt><%= label :promotion, :description %></dt>
					<dd><%= @publish.promotion.description %>&nbsp;</dd>
				</dl>
				<dl>
					<% if @promotion.click_reward %>
						<dt><%= label :promotion, :click_reward %></dt>
						<dd><%= @promotion.click_reward.price %>円</dd>
					<% end %>
					<% if false %>
						<% if @promotion.reward %>
							<dt><%= label :promotion, :reward %></dt>
							<dd><%= @promotion.reward.price %>円</dd>
						<% end %>
					<% end %>
				</dl>
			</div>
			<h3>サイト詳細</h3>
			<div id="site_attributes" class="siteDetails">
				<dl>
					<dt><%= label :site, :name %></dt>
					<dd><%= @publish.site.name %></dd>
					<dt><%= label :site,	:url %></dt>
					<dd><%= @publish.site.url %></dd>
				</dl>
				<dl class="aspName">
					<dt><%= label :site, :description %></dt>
					<dd><%= @publish.site.description %>&nbsp;</dd>
				</dl>
				<dl>
					<dt><%= label :site, :keywords %></dt>
					<dd><%= @publish.site.keywords %></dd>
				</dl>
			</div>
			<div class="actions one">
				<span class="tieUpOn"><%= link_to "提携解除", polymorphic_path([:partner, @publish], :action => :delete) %></span>
			</div>
		</div>
		<!-- /#tab -->
		<!-- #tab -->
		<div id="tabReward">
			<div class="tab_contents">
				<%= render :partial => "banners" %>
			</div>
			<div class="actions one">
				<span class="back"><%= link_to "戻る", polymorphic_path([:partner, :publishes]) %></span>
			</div>
		</div>
		<!-- /#tab -->
	</div>
	<!-- /#jQTabs -->
</div>
<!-- /.board -->
