<h2>ASP一覧</h2>
<canvas id="canvas"><!-- ASP一覧 --></canvas>

<p class="<%= @client.asps.count > 0 ? "actions head" : "actions one head" %>">
	<span class="back"><%= link_to "戻る", polymorphic_path([:client, @promotion]) %></span>
	<% if @client.asps.count > 0 %>
	<span class="edit"><%= link_to "提携ASP編集", edit_polymorphic_path([:client, @promotion, :one_tags]) %></span>
	<% end %>
</p>
<div id="attributes">
	<dl class="conversionTag">
		<dt><%= label_tag "提携ASP" %></dt>
		<dd>
			<ul>
				<% if @asps.size < 1 %><li><span>選択されていません。</span></li>
				<% end %>
				<% @asps.each do |item| %>
				<li><%= item.name %></li>
				<% end %>
			</ul>
		</dd>
	</dl>
</div>
<p class="<%= @client.asps.count > 0 ? "actions fot" : "actions one fot" %>">
	<span class="back"><%= link_to "戻る", polymorphic_path([:client, @promotion]) %></span>
	<% if @client.asps.count > 0 %>
	<span class="edit"><%= link_to "提携ASP編集", edit_polymorphic_path([:client, @promotion, :one_tags]) %></span>
	<% end %>
</p>